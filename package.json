{
    "name": "commons-mcp",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "MCP server for searching Wikimedia Commons images.",
    "scripts": {
        "build": "tsc -p .",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "lint": "eslint .",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "tsx --test"
    },
    "engines": {
        "node": ">=18.17.0"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.1.1",
        "cross-fetch": "^4.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.14.9",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "prettier": "^3.3.3",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4"
    }
}