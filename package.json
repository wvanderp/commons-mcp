{
    "name": "commons-mcp",
    "version": "1.3.0",
    "type": "module",
    "description": "Model Context Protocol server for searching Wikimedia Commons images.",
    "keywords": [
        "mcp",
        "model-context-protocol",
        "wikimedia",
        "commons",
        "images",
        "search"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wvanderp/commons-mcp.git"
    },
    "bugs": {
        "url": "https://github.com/wvanderp/commons-mcp/issues"
    },
    "homepage": "https://github.com/wvanderp/commons-mcp#readme",
    "bin": {
        "commons-mcp": "./dist/index.js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p .",
        "prepublishOnly": "npm run lint && npm test && npm run build",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "lint": "eslint ./src ./test --ext .ts",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "tsx --test"
    },
    "engines": {
        "node": ">=18.17.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.17.4",
        "cross-fetch": "^4.1.0",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@types/node": "^24.3.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "@typescript-eslint/parser": "^8.41.0",
        "@typescript-eslint/eslint-plugin": "^8.41.0",
        "eslint-import-resolver-typescript": "^4.4.4",
        "prettier": "^3.6.2",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    }
}
